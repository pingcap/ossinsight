{
  "name": "@ossinsight/api-server",
  "version": "1.0.0",
  "description": "The API server of OSSInsight.",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "build:ts": "tsc",
    "dev": "NODE_ENV=development fastify start -p ${API_SERVER_PORT:-3450} -P --ignore-watch=.ts$ -L dist/logger.js -l info -w dist/app.js",
    "build": "rm -rf dist && npm run build:ts",
    "start": "fastify start -a 0.0.0.0 -p ${API_SERVER_PORT:-3450} -L dist/logger.js -l info dist/app.js",
    "gen:dev-public-api-docs": "fastify start -a 0.0.0.0 -p ${API_SERVER_PORT:-3450} -L dist/logger.js -l info dist/cmd/gen-docs.js",
    "gen:public-api-docs": "rm -f ../../configs/public_api/openapi.yaml && API_BASE_URL=https://api.ossinsight.io fastify start -a 0.0.0.0 -p ${API_SERVER_PORT:-3450} -L dist/logger.js -l info dist/cmd/gen-docs.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/env": "^4.1.0",
    "@fastify/etag": "^4.2.0",
    "@fastify/forwarded": "^2.2.0",
    "@fastify/mysql": "^4.1.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/redis": "^6.1.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.7.0",
    "@fastify/url-data": "^5.3.1",
    "@fastify/websocket": "^7.1.1",
    "@lukesthl/ts-axios-digest-auth": "^1.0.5",
    "@mgcrea/fastify-request-logger": "^0.6.1",
    "@ossinsight/types": "workspace:^",
    "@sentry/node": "^7.57.0",
    "JSONStream": "^1.3.5",
    "async": "^3.2.5",
    "axios": "^1.6.7",
    "bullmq": "^3.4.1",
    "fastify": "^4.0.0",
    "fastify-auth0-verify": "^1.0.0",
    "fastify-cli": "^5.5.1",
    "fastify-metrics": "^10.0.0",
    "fastify-plugin": "^4.3.0",
    "fastify-socket.io": "^4.0.0",
    "generic-pool": "^3.9.0",
    "ioredis": "^5.2.4",
    "jsonrepair": "^3.0.2",
    "liquidjs": "^10.9.2",
    "luxon": "^3.1.0",
    "mustache": "^4.2.0",
    "mysql2": "^2.3.3",
    "node-sql-parser": "4.6.5",
    "octokit": "^3.1.2",
    "openai": "^3.3.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "pino-sentry-transport": "^1.0.4",
    "prom-client": "^14.1.0",
    "promised-sqlite3": "^2.1.0",
    "socket.io": "^4.5.3",
    "sqlite3": "^5.1.6",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@fastify/type-provider-json-schema-to-ts": "^2.1.1",
    "@types/async": "^3.2.24",
    "@types/chance": "^1.1.3",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/luxon": "^3.0.2",
    "@types/mustache": "^4.2.2",
    "@types/node": "^18.0.0",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^2.1.1",
    "chance": "^1.1.9",
    "fastify-tsconfig": "^1.0.1",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "json-schema-to-ts": "^2.6.0",
    "json-schema-to-typescript": "^11.0.2",
    "randexp": "^0.5.3",
    "socket.io-client": "^4.5.3",
    "testcontainers": "^9.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
