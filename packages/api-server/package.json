{
  "name": "@ossinsight/api-server",
  "version": "1.0.0",
  "description": "The API server of OSSInsight.",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "build:ts": "tsc",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start -p ${API_SERVER_PORT:-3450} -P --ignore-watch=.ts$ -l info -w dist/app.js",
    "build": "rm -rf dist && npm run build:ts",
    "start": "fastify start -a 0.0.0.0 -p ${API_SERVER_PORT:-3450} -l info dist/app.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "@fastify/auth": "^4.2.0",
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/env": "^4.1.0",
    "@fastify/jwt": "5.0.0",
    "@fastify/mysql": "^4.0.0",
    "@fastify/oauth2": "^6.1.0",
    "@fastify/redis": "^6.1.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.1.0",
    "@fastify/websocket": "^7.1.1",
    "@mgcrea/fastify-request-logger": "^0.6.1",
    "@octokit/core": "^4.1.0",
    "@octokit/graphql": "^5.0.4",
    "@octokit/plugin-throttling": "^4.3.2",
    "@octokit/rest": "^19.0.5",
    "axios": "^0.27.2",
    "bullmq": "^3.4.1",
    "fastify": "^4.0.0",
    "fastify-auth0-verify": "^1.0.0",
    "fastify-cli": "^5.5.1",
    "fastify-metrics": "^10.0.0",
    "fastify-plugin": "^4.3.0",
    "fastify-socket.io": "^4.0.0",
    "generic-pool": "^3.9.0",
    "ioredis": "^5.2.4",
    "luxon": "^3.1.0",
    "mysql2": "^2.3.3",
    "node-sql-parser": "4.5.1",
    "octokit": "^2.0.10",
    "openai": "^3.1.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "prom-client": "^14.1.0",
    "socket.io": "^4.5.3",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@fastify/type-provider-json-schema-to-ts": "^2.1.1",
    "@types/chance": "^1.1.3",
    "@types/generic-pool": "^3.1.11",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/luxon": "^3.0.2",
    "@types/node": "^18.0.0",
    "@types/ws": "^8.5.3",
    "chance": "^1.1.9",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "json-schema-to-ts": "^2.6.0",
    "randexp": "^0.5.3",
    "socket.io-client": "^4.5.3",
    "testcontainers": "^9.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "types-fastify-socket.io": "^0.0.1",
    "typescript": "^4.5.4"
  }
}
